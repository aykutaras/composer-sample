{
    "name": "aykutaras/composer-sample",
    "description": "Composer packaging sample repository",
    "authors": [
        {
            "name": "Aykut Aras",
            "email": "aykutaras@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/aykutaras/composer-sample"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3"
    },
    "autoload": {
        "psr-4": {
            "AykutAras\\ComposerSample\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AykutAras\\ComposerSample\\Test\\": "tests/"
        }
    }
}
